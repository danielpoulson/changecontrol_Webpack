{
  "name": "changecontrol",
  "version": "2.0.0",
  "description": "Change conrol application",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --progress --verbose --colors --display-error-details --config webpack/common.config.js",
    "build:production": "npm run clean && npm run build",
    "lint": "node_modules/.bin/esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "NODE_ENV=development node server.js",
    "startProduction": "NODE_ENV=production node bin/server.js",
    "test": "karma start",
    "test:watch": "node bin/testMessage",
    "dpGit": "git add . && git commit -m 'eslint / refactor' && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielpoulson/changecontrol.git"
  },
  "license": "MIT",
  "devDependencies": {
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.3.5",
    "crypto": "0.0.3",
    "ejs": "^2.3.3",
    "express": "^4.13.3",
    "express-csv": "^0.6.0",
    "express-session": "^1.11.3",
    "json2csv": "^3.2.0",
    "lodash": "^3.10.1",
    "moment": "^2.12.0",
    "mongoose": "^4.4.11",
    "mongoose-to-csv": "^0.1.0",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "nodemailer": "^2.3.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0"
  }
}
